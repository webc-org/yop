{
  "name": "@pushui/styles",
  "version": "0.0.10",
  "description": "Push UI design system styles",
  "main": "dist/index.css",
  "files": [
    "dist",
    "styles"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build": "sass styles/index.scss dist/index.css --style=compressed && sass styles/index.scss dist/index.expanded.css",
    "dev": "sass styles/index.scss dist/index.css --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "sass": "^1.83.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webc-org/pushui.git",
    "directory": "packages/styles"
  },
  "license": "MIT",
  "dependencies": {
    "lucide-static": "^0.563.0"
  }
}
